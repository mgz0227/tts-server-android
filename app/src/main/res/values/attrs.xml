<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="max" format="integer" />
    <attr name="progress" format="integer" />
    <attr name="hint" format="string|reference" />

    <declare-styleable name="Seekbar">
        <attr name="min" format="integer" />
        <attr name="max" />
        <attr name="progress" />
        <attr name="hint" />
    </declare-styleable>

    <declare-styleable name="AppSlider">
        <attr name="stepSize" format="float" />
        <attr name="valueTo" format="float" />
        <attr name="valueFrom" format="float" />
        <attr name="value" format="float" />
        <attr name="floatN" format="integer" />
        <attr name="hint" />
    </declare-styleable>

    <declare-styleable name="MaterialSpinner">
        <attr name="emptyText" format="reference|string" />
    </declare-styleable>
</resources>